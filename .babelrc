{
    "presets": [
        [
            "@babel/env",
            {
                "targets": {
                    "node": 6
                }
            }
        ]
    ],
    "plugins": [
        "@babel/transform-runtime"
    ],
    "sourceMaps": true,
    "retainLines": true,
    "env": {
        "test": {
            "plugins": [
                "istanbul"
            ]
        }
    }
}
